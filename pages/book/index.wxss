
.container{display: flex;flex-direction: column;align-items: center;width: 100%}
.sub-container{display: flex;flex-direction: column;align-items: center;background: #f5f5f5;margin-top: 100rpx;}
.box{display: flex;flex-direction:row;justify-content: center;border-radius:50px;background:#f5f5f5;height: 68rpx;width:700rpx;color: #999;align-items: center;margin:10rpx auto;box-shadow: 0 0 3px #e3e3e3}
.header{position: fixed;background: #fff;height: 100rpx;width: 100%;border-top:1px solid #f5f5f5;z-index: 99;}
.box image{width: 14px;height: 14px;margin-right: 20rpx;margin-bottom: -2px}
.head-img{width:106rpx;height: 34rpx;margin-top: 30rpx}
.container-book{display: flex;flex-direction: row;flex-wrap: wrap;padding: 0 90rpx 0 90rpx;justify-content: space-between}
.container-book v-book{margin-bottom: 10rpx}